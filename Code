  {
   "cell_type": "code",
   "execution_count": 13,
   "id": "ec31a197-d2bd-43b3-a753-7f30d79bd270",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Sum of elements in w: 0.5187709081074007\n"
     ]
    }
   ],
   "source": [
    "# Select all cars from Asia\n",
    "asia_cars = df[df['origin'] == 'Asia']\n",
    "\n",
    "# Select only columns vehicle_weight and model_year\n",
    "subset = asia_cars[['vehicle_weight', 'model_year']]\n",
    "\n",
    "# Select the first 7 values\n",
    "subset7 = subset.head(7)\n",
    "\n",
    "# Get underlying NumPy array\n",
    "X = subset7.to_numpy()\n",
    "\n",
    "# Compute XTX = X.T @ X\n",
    "XTX = X.T @ X\n",
    "\n",
    "# Invert XTX\n",
    "XTX_inv = np.linalg.inv(XTX)\n",
    "\n",
    "# Create array y\n",
    "y = np.array([1100, 1300, 800, 900, 1000, 1100, 1200])\n",
    "\n",
    "# Compute w = (XTX_inv @ X.T) @ y\n",
    "w = (XTX_inv @ X.T) @ y\n",
    "\n",
    "# Sum of elements in w\n",
    "result = w.sum()\n",
    "print(\"Sum of elements in w:\", result)\n"
   ]
  },
